<sparql xmlns="http://www.w3.org/2005/sparql-results#"
        xml:base="http://revelytix.com/base/foo">
  <head xml:base="/ns/bar">
    <variable name="x"/>
    <variable name="y"/>
    <link href="http://sample.org/metadata.rdf"/>
    <link href="service-description.rdf"/>
    <link xml:base="http://example.com/" href="sparql.ttl"/>
  </head>
